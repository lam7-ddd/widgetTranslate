{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/widget\\.(js|css)",
      "dest": "/client/widget.$1"
    },
    {
      "src": "/test/(.*)",
      "dest": "/test/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server/index.js"
    }
  ]
}
