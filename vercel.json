{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "test/**",
      "use": "@vercel/static"
    },
    {
      "src": "client/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/widget\\.(js|css)",
      "dest": "/client/widget.$1"
    },
    {
      "src": "/test/(.*\\.(html|css|js))",
      "dest": "/test/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/server/index.js"
    },
    {
      "src": "/health",
      "dest": "/server/index.js"
    },
    {
      "src": "/",
      "dest": "/server/index.js"
    }
  ]
}
