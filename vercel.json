{
  "version": 2,
  "functions": {
    "server/index.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/widget\\.(js|css)",
      "dest": "/client/widget.$1"
    },
    {
      "src": "/test/(.*)",
      "dest": "/test/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
